{
  "name": "secure-registration-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --max-warnings 0",
    "format": "prettier '**/*.{js,jsx,ts,tsx}' --write",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:registration": "cypress run --spec \"cypress/e2e/registration/*\"",
    "cypress:login": "cypress run --spec \"cypress/e2e/login/*\"",
    "test:all-browsers": "node run-tests-on-all-browsers.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "axios": "^1.6.7",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.22.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-vitest": "^0.3.26",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "history": "^5.3.0",
    "jsdom": "^24.0.0",
    "pa11y": "^7.0.0",
    "prettier": "^3.2.5",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
